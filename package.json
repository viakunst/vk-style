{
  "name": "vk-style",
  "version": "1.0.0",
  "description": "Common React components for ViaKunst",
  "author": "viakunst",
  "license": "Apache-2.0",
  "repository": "viakunst/vk-style",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "types": "dist/index.d.ts",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "cross-env CI=1 react-scripts test --env=jsdom",
    "lint": "run-s lint:js lint:css",
    "lint:quiet": "run-s lint:js:quiet lint:css:quiet",
    "lint:fix": "run-s lint:js:fix lint:css:fix",
    "lint:js": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:js:quiet": "eslint src/**/*.{js,jsx,ts,tsx} --quiet",
    "lint:js:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "lint:css": "stylelint src/**/*.{scss,css}",
    "lint:css:quiet": "stylelint src/**/*.{scss,css} --quiet",
    "lint:css:fix": "stylelint src/**/*.{scss,css} --fix",
    "analyse": "tsc --noemit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-interactions": "^6.4.20",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.4",
    "cross-env": "^7.0.2",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-storybook": "^0.5.7",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "stylelint": "^14.6.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.6.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {}
}
